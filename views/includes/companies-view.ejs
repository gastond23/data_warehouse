<table id="table" class="table table-striped table-sm table-bordered">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Nombre</th>
            <th scope="col">País</th>
            <th scope="col">Dirección</th>
            <th scope="col">Teléfono</th>
            <th scope="col">Acción</th>
        </tr>
    </thead>
    <tbody>
        <% for (let company of data) { %>
        <tr>
            <th scope="row" class="align-middle"><%= company.id %> </th>
            <td class="align-middle"><%= company.name %></td>
            <td class="align-middle"><%= company.city.country.name %></td>
            <td class="align-middle"><%= company.adress %></td>
            <td class="align-middle"><%= company.phone %></td>
            <td class="align-middle">
                <button type="button" class="btn btn-primary">Editar</button>
                <button type="button" class="btn btn-danger">Borrar</button>
            </td>
        </tr>
        <% }; %>
    </tbody>
</table>