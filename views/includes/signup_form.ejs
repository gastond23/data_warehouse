<div class="main-signup">
    <h2 id="form-title">Crear usuario</h2>
    <form class="signup-form">
        <div class="form-group">
            <label for="validationServer01">First name</label>
            <input type="text" class="form-control mx-sm-3" id="validationServer01" required>
        </div>
        <div class="form-group">
            <label for="validationServer02">Last name</label>
            <input type="text" class="form-control mx-sm-3" id="validationServer02" required>
        </div>
        <div class="form-group">
            <label for="inputEmail3">Email</label>
            <input type="email" class="form-control mx-sm-3" id="inputEmail3">
        </div>
        <label for="customSwitch2">Perfil</label>
        <div class="form-group custom-control custom-switch">
            <input type="checkbox" class="custom-control-input" id="customSwitch2">
            <label class="custom-control-label" for="customSwitch2">Off User / On Admin</label>
        </div>
        <div class="form-group">
            <label for="inputPassword6">Password</label>
            <input type="password" id="inputPassword6" class="form-control mx-sm-3"
                aria-describedby="passwordHelpInline1">
            <small id="passwordHelpInline" class="text-muted">
                Must be 8-20 characters long.
            </small>
        </div>
        <div class="form-group">
            <label for="inputPassword67">Repeat Password</label>
            <input type="password" id="inputPassword67" class="form-control mx-sm-3"
                aria-describedby="passwordHelpInline2">
            <small id="passwordHelpInline" class="text-muted">
                Password must be match.
            </small>
        </div>
        <button class="btn btn-primary" id="signup_btn">Submit</button>
    </form>
</div>