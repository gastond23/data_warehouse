<div id="jstree" class="tree-container">
    <ul> <% for (let region of data) { %>
        <li id="regionId <%= region.id %>" data-jstree='{"icon":"https://icons.getbootstrap.com/icons/globe.svg"}'>
            <%= region.name %>
            <ul>
                <% for (let country of region.countries) { %>
                <li id="countryId <%= country.id %> " data-jstree='{"icon":"https://icons.getbootstrap.com/icons/flag-fill.svg"}'><%= country.name %>
                    <ul>
                        <% for (let city of country.cities) {%>
                        <li id="cityId <%= city.id %>" data-jstree='{"icon":"https://icons.getbootstrap.com/icons/geo-alt.svg"}'><%= city.name %></li>
                        <% }; %>
                    </ul>
                </li>
                <% }; %>
            </ul>
        </li>
        <% }; %>
    </ul>
</div>